{
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/", "destination": "/dashboard" },
    { "source": "/index.html", "destination": "/dashboard" }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [{ "key": "x-wallboard", "value": "spotter" }] }
  ]
}
